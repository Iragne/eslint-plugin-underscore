{
    "env": {
        "node": true
    },
    "rules": {
        "accessor-pairs": 2,
        "no-array-constructor": 2,
        "no-catch-shadow": 2,
        "comma-dangle": 2,
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "no-cond-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-else-return": 2,
        "no-empty-character-class": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-extend-native": 2,
        "no-extra-boolean-cast": 2,
        "no-unneeded-ternary": 2,
        "strict": [2, "global"],
        "no-inner-declarations": [2, "functions"],
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 2,
        "no-new-require": 2,
        "no-octal-escape": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-modules": 2,
        "no-script-url": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "semi-spacing": [2, {"before": false, "after": true}],
        "no-sparse-arrays": 2,
        "no-undef": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
        "yoda": 2,

        "block-scoped-var": 2,
        "brace-style": [2, "1tbs", {"allowSingleLine": true}],
        "consistent-return": 2,
        "consistent-this": [2, "self"],
        "curly": [2, "all"],
        "func-style": [2, "declaration"],
        "space-after-keywords": [2, "always"],
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-in-parens": [2, "never"],
        "padded-blocks": [2, "never"],
        "valid-typeof": 2,

        "no-alert": 2,
        "no-caller": 2,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extra-semi": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-implied-eval": 2,
        "no-with": 2,
        "no-fallthrough": 2,
        "no-unreachable": 2,
        "no-undef-init": 2,
        "no-octal": 2,
        "no-obj-calls": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-native-reassign": 2,
        "no-delete-var": 2,
        "no-return-assign": 2,
        "no-new-object": 2,
        "no-label-var": 2,
        "no-self-compare": 2,
        "no-use-before-define": [2, "nofunc"],
        "no-mixed-spaces-and-tabs": [2, true],
        "no-extra-bind": 2,

        "dot-notation": 2,
        "eqeqeq": 2,
        "new-parens": 2,
        "guard-for-in": 2,
        "radix": 2,
        "new-cap": 2,
        "semi": 2,
        "use-isnan": 2,
        "quotes": [2, "single", "avoid-escape"],
        "wrap-iife": 2,
        "no-multi-str": 2,

        "quote-props": [2, "as-needed"],
        "no-multi-spaces": 2,
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "comma-spacing": 2,
        "space-unary-ops": [2, {"words": true, "nonwords": false}],
        "indent": [2, 4, {"SwitchCase": 1}],
        "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
        "space-before-blocks": [2, "always"],
        "space-before-keywords": [2, "always"],
        "operator-linebreak": [2, "after"],
        "dot-location": [2, "property"],
        "array-bracket-spacing": [2, "never"],
        "object-curly-spacing": [2, "never"],
        "no-throw-literal": 2,
        "no-useless-concat": 2
    }
}
