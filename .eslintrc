{
    "env": {
        "node": true
    },
    "rules": {
        "comma-dangle": 2,
        "indent": [2, 4, {"SwitchCase": 1}],
        "no-extra-parens": 0,
        "no-multiple-empty-lines": [2, {"max": 1}],
        "no-undef": 2,
        "no-unused-vars": 2,
        "object-curly-spacing": 2,
        "quotes": [2, "single", "avoid-escape"],
        "semi": 2,
        "strict": [2, "global"]
    }
}
